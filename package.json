{
  "name": "frontend_web",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "buildwCSS": "npm run cssProd; npm run build",
    "start": "vite --host --port 3000",
    "build": "tsc && vite build --minify",
    "css": "tailwind -o public/assets/css/output.css",
    "cssProd": "tailwind -c tailwind.prod.js -o public/assets/css/output.css",
    "preview": "vite preview",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@tinymce/tinymce-react": "^5.1.1",
    "prettier": "2.7.1",
    "quill": "^2.0.2",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-hook-form": "^7.53.0",
    "react-quill": "^2.0.0",
    "react-router-dom": "^6.3.0",
    "tailwindcss": "^2.2.4",
    "tinymce": "^7.3.0"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.3.3",
    "@types/node": "^17.0.41",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/react-router-dom": "^5.3.3",
    "@types/tailwindcss": "^2.2.1",
    "@vitejs/plugin-react": "^1.3.0",
    "autoprefixer": "^10.2.6",
    "postcss": "^8.3.5",
    "postcss-cli": "^8.3.1",
    "postcss-import": "^14.0.2",
    "typescript": "^4.6.3",
    "vite": "^2.9.7",
    "vitest": "^0.10.2"
  }
}
